<Window x:Class="Modules.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:infra="clr-namespace:Modules.Infrastructure;assembly=Modules.Infrastructure"
        xmlns:local="clr-namespace:Modules.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="900">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        
        <ContentControl Grid.Column="0" prism:RegionManager.RegionName="{x:Static infra:RegionNames.LeftRegion}" />
        <ContentControl Grid.Column="1" prism:RegionManager.RegionName="{x:Static infra:RegionNames.RightRegion}" />
    </Grid>
</Window>
